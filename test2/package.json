{
  "name": "test2",  // 프로젝트 이름(우리가 신경쓸 부분)
  "version": "0.1.0", // 버전 정보(우리가 신경쓸 부분)
  "private": true,
  "dependencies": { // 종속성(우리가 신경쓸 부분)
    "@testing-library/jest-dom": "^5.16.5", // 뒤에 숫자들은 버전 정보이고 ^는 이 버전 이상 사용 가능
